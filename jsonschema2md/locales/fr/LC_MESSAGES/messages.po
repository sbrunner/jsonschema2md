#, python-format
msgid "Length must be at least %(min)d."
msgstr "La longueur doit être d'au moins %(min)d."

#, python-format
msgid "Length must be at most %(max)d."
msgstr "La longueur doit être au maximum de %(max)d."

#, python-format
msgid "Length must be equal to %(length)d."
msgstr "La longueur doit être égale à %(length)d."

#, python-format
msgid "Length must be between %(min)d and %(max)d (inclusive)."
msgstr "La longueur doit être comprise entre %(min)d et %(max)d (inclus)."

msgid "Must be an integer."
msgstr "Doit être un entier."

#, python-format
msgid "Must be a multiple of `%(multiple)d`."
msgstr "Doit être un multiple de `%(multiple)d`."

