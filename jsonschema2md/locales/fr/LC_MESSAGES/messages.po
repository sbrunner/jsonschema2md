#, python-format
msgid "Must be of type *%(type)s*."
msgstr "Doit être de type *%(type)s*."

#, fuzzy, python-format
msgid "Content encoding: `%(encoding)s`."
msgstr "Encodage du contenu : `%(encoding)s`."

#, python-format
msgid "Content media type: `%(type)s`."
msgstr "Type de média du contenu : `%(type)s`."

#, python-format
msgid "Minimum: `%(min)d`."
msgstr "Minimum : `%(min)d`."

#, python-format
msgid "Exclusive minimum: `%(exmin)d`."
msgstr "Minimum exclusif : `%(exmin)d`."

#, python-format
msgid "Maximum: `%(max)d`."
