msgstr "obrigatório <sub><sup>se %(dependent)s está definido</sup></sub>"

msgid "deprecated"
msgstr "obsoleto"

msgid "read-only"
msgstr "somente-leitura"

msgid "write-only"
msgstr "somente-escrita"

#, python-format
msgid ", %(attributes)s"
msgstr ", %(attributes)s"

